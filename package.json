{
  "name": "tag-counter",
  "version": "1.0.0",
  "description": "A simple Node.js CLI application",
  "keywords": [
    "asynchronous",
    "cli-application",
    "es2015",
    "javascript",
    "json-parsing",
    "nodejs"
  ],
  "homepage": "https://github.com/ajtorres9/tag-counter#readme",
  "bugs": "https://github.com/ajtorres9/tag-counter/issues",
  "license": "MIT",
  "author": "Andrew Torres <andrew.jonathan.torres@gmail.com> (https://andrewjtorr.es)",
  "bin": "./cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajtorres9/tag-counter.git"
  },
  "scripts": {
    "lint": "yarn lint-files && yarn lint-scripts",
    "lint-files": "prettylint --ignore-path .gitignore **/*.{json,md,yml}",
    "lint-scripts": "eslint --cache --ignore-path .gitignore --ignore-pattern !**/.* **/*.js",
    "test": "yarn test-units",
    "test-units": "nyc ava"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "ava": "^2.0.0",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^9.0.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettylint": "^1.0.0",
    "sinon": "^7.3.2"
  },
  "engines": {
    "node": ">=8.16.0",
    "yarn": ">=1.16.0"
  },
  "private": true
}
